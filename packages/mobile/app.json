{
  "expo": {
    "scheme": "migoculto",
    "name": "MigOculto",
    "slug": "migoculto",
    "runtimeVersion": "2.0.0",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
