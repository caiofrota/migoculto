    "newArchEnabled": true,
    "ios": {
      "buildNumber": "16",
      "supportsTablet": true,
      "bundleIdentifier": "com.cftechsol.migoculto",
      "usesAppleSignIn": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Este aplicativo usa c√¢mera para escanear QR codes para entrar em um grupo.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "monochromeImage": "./assets/images/adaptive-icon.png",
        "backgroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#8b0000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.cftechsol.migoculto",
      "versionCode": 16
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f954a9f2-1474-4fc1-b003-a0b90f54f4ee"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
